{
  "name": "venda_server",
  "version": "1.0.0",
  "description": "venda server for admin, vendor, customer and rider apps",
  "keywords": [
    "venda"
  ],
  "license": "ISC",
  "author": "ezeani chucks",
  "type": "commonjs",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "test": "jest",
    "start": "node dist/src/index.js",
    "dev": "npx tsc && nodemon src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migrate:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/$npm_config_name -d src/config/data-source.ts",
    "migrate:run": "typeorm-ts-node-commonjs migration:run -d src/config/data-source.ts",
    "migrate:revert": "typeorm-ts-node-commonjs migration:revert -d src/config/data-source.ts",
    "migrate:create": "typeorm-ts-node-commonjs migration:create src/migrations/$npm_config_name",
    "migration:show": "npm run typeorm migration:show -- -d src/config/data-source.ts"
  },
  "dependencies": {
    "africastalking": "^0.7.8",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "cloudinary": "^2.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.3.0",
    "firebase-admin": "^13.6.0",
    "geojson": "^0.5.0",
    "helmet": "^8.1.0",
    "install": "^0.13.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "npm": "^11.6.4",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "typeorm": "^0.3.27",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "nodemon": "^3.1.9",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
